[{"name":"server.R","content":"# The server file\nfunction(input, output) {\n    \n    observe({\n        data = read.csv(\"Exam1.csv\")\n        # render the histogram in this function\n        output$main_plot <- renderPlot({\n            \n            hist(data$Score,\n                 probability = TRUE,\n                 breaks = as.numeric(input$n_breaks),\n                 xlab = \"Scores\",\n                 main = paste(\"Exam1\", \"Exam Scores Distribution\")\n                 )\n            # add the density line if selected\n            if (input$density) {\n                dens <- density(data$Score, adjust = input$bw_adjust)\n                lines(dens, col = \"blue\")\n            }\n        })\n        \n        # get the file typed in from the user\n        data_file_2 = input$file_2\n        if (is.null(data_file_2)) {\n            return(NULL)\n        }\n        # read in the file\n        data_2 = read.csv(data_file_2$datapath)\n        # render the histogram in this function\n        output$main_plot_2 <- renderPlot({\n            \n            hist(data_2$Score,\n                 probability = TRUE,\n                 breaks = as.numeric(input$n_breaks),\n                 xlab = \"Scores\",\n                 main = paste(data_file_2$name, \"Exam Scores Distribution\")\n            )\n            # add the density line if selected\n            if (input$density) {\n                dens_2 <- density(data_2$Score, adjust = input$bw_adjust)\n                lines(dens_2, col = \"blue\")\n            }\n        })\n    })\n}\n","type":"text"},{"name":"ui.R","content":"# The user interface\nfluidPage(\n \n    # Application title\n    titlePanel(\"Exam Score Distributions\"),\n    \n    sidebarLayout(\n        sidebarPanel(\n            selectInput(inputId = \"n_breaks\",\n                        label = \"Number of bins in histogram (approximate):\",\n                        choices = c(10, 20, 35, 50),\n                        selected = 20),\n            tags$hr(),\n            \n            checkboxInput(inputId = \"density\",\n                          label = strong(\"Show density estimate\"),\n                          value = FALSE),\n            \n            tags$hr(),\n            \n            # display this only if the density is selected to be shown\n            conditionalPanel(condition = \"input.density == true\",\n                             sliderInput(inputId = \"bw_adjust\",\n                                         label = \"Bandwidth adjustment:\",\n                                         min = 0.2, max = 2, value = 1, step = 0.2)\n            )\n        ),\n\n        mainPanel(\n            tags$hr(), # an html tag for a horizontal rule, hr\n            \n            # where the plot will be rendered\n            plotOutput(outputId = \"main_plot\", height = \"300px\"),\n            \n            # Second File Chooser\n            fileInput('file_2', 'Select the exam csv file',\n                      accept = c('text/csv','text/comma-separated-values,text/plain','.csv')),\n            \n            tags$hr(), # an html tag for a horizontal rule, hr\n            \n            # where the plot will be rendered\n            plotOutput(outputId = \"main_plot_2\", height = \"300px\")\n        )\n    )\n)\n","type":"text"},{"name":"Exam1.csv","content":"Score\r8.79\r12.53\r15.38\r17.58\r17.58\r23.08\r26.37\r27.47\r30.33\r31.43\r31.65\r33.27\r33.3\r34.51\r35.16\r35.16\r35.71\r36.04\r36.04\r36.26\r36.81\r37.58\r37.58\r38.24\r38.46\r39.34\r39.56\r40.09\r40.11\r40.66\r40.66\r40.66\r41.1\r41.1\r42.42\r43.07\r43.08\r43.96\r44.18\r44.18\r44.18\r44.62\r45.16\r45.27\r45.27\r45.49\r45.71\r45.93\r46.15\r46.37\r47.03\r47.91\r48.35\r48.35\r48.46\r48.55\r48.57\r49.01\r49.01\r49.45\r49.45\r49.45\r49.45\r49.77\r49.78\r50.11\r50.33\r50.55\r50.55\r50.77\r50.77\r51.43\r51.54\r51.65\r51.65\r51.74\r52.09\r52.09\r52.09\r52.31\r53.08\r53.08\r53.35\r53.85\r53.85\r54.07\r54.4\r54.51\r54.95\r54.95\r55.16\r55.38\r56.04